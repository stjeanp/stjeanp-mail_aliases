{
  "name": "stjeanp-mail_aliases",
  "version": "0.3.1",
  "author": "Patrick St. Jean",
  "summary": "Manages email aliases on Linux based systems via hiera data",
  "license": "Apache-2.0",
  "source": "https://github.com/stjeanp/stjeanp-mail_aliases",
  "project_page": "https://github.com/stjeanp/stjeanp-mail_aliases",
  "issues_url": "https://github.com/stjeanp/stjeanp-mail_aliases/issues",
  "tags": [
    "aliases"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "OracleLinux",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Scientific",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8",
        "9",
        "10"
      ]
    },
    {
      "operatingsystem": "OpenSuSE",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 5.5.0 < 7.0.0"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 2.0.0 < 7.0.0"
    },
    {
      "name": "puppetlabs/mailalias_core",
      "version_requirement": ">= 1.0.0 < 2.0.0"
    }
  ]
}
